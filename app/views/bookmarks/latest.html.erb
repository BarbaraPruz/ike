<h1>Latest Bookmarks</h1>
<table class="table">
    <thead>
        <tr>
            <th scope="col">Created</th>
            <th scope="col">Title</th>
            <th scope="col">User</th>
        </tr>
    </thead>
    <tbody>
        <% @bookmarks.each do | bookmark | %>
            <tr>
                <td><%=format_time(bookmark.created_at)%></td>
                <td><%= bookmark.title %></td>
                <td><%= bookmark.username %></td>
            </tr>
    <% end %>
</ul>